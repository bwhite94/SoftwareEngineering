@model FindTheBooty.Models.GeneratedModels.hunt
@{
    var HuntID = Model.hunt_id;
    var HuntName = Model.hunt_name;
    ViewBag.Title = "You have joined a hunt!";
}

<div class="row">
    <div class="md-col-12 col-sm-12 col-xs-12">

        <h2 class="kaushanScript">You've joined a hunt!</h2>

        <p>You have joined the "@HuntName" Hunt!</p>

        <h3>What's next?</h3>
        <a href="/Participating/DoHunt/@HuntID" class="btn btn-success">Continue to Hunt</a>
        <a href="/" class="btn btn-default">Return to Dashboard</a>

    </div>
</div>